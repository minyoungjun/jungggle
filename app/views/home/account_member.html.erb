<div class="account">
  <div class="container">
    <%=render "left"%>
    <div class="right-member">
      <h1>Member</h1>
      <h3>Gameberry Inc.</h3>
      <table>
        <tr class="table-header">
          <td>Name</td>
          <td>Email</td>
          <td>Privlleges</td>
        </tr>
        <% current_user.member.company.members.each do |member|%>
        <tr class="table-body">
          <td><%=member.user.first_name%> <%=member.user.last_name%></td>
          <td class="email"><%=member.user.email%></td>
          <td><img src="/assets/my_account/person_1_gray.png"><%=member.privilege%></td>
        </tr>
        <%end%>
      </table>
      <h3>Wailting for Approval</h3>
      <table>
        <tr class="table-header">
          <td>Name</td>
          <td>Email</td>
          <td>Access</td>
        </tr>
        <tr class="table-body">
          <td>Peter Carpenter</td>
          <td class="email">bill_willian@gmail.com</td>
          <td>
            <button type="button" class="btn btn-left">Approve</button>
            <button type="button" class="btn btn-right">Disapprove</button>
          </td>
        </tr>
      </table>
    </div>
  </div>
</div>




