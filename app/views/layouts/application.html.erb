</head>
<body>
  <div class="header">
    <div class="container">
      <a href="/home/main" class="logo"><img src="/assets/main/header-logo.png"<%if @main%>style="opacity:0;"<%end%>></a>
      <div class="header-search">
        <span class="glyphicon glyphicon-search"></span>
        <input type="search" placeholder="Search Company" autofocus autocomplete="off" id="search_title" onchange="search_auto_func();" onkeyup="search_auto_func();" name="search_title" >
        <div class="search-drop search-company-name-drop" style="display:none;">
          <div class="search-company search_auto_div" data-company="App flydgd Inc.">
            <img src="/assets/product_card_view/02.jpg"><h5>App flydgd Incaaaaaaaa.</h5>
          </div>
          <div class="search-company search_auto_div" data-company="App fgasgas Inc.">
            <img src="/assets/product_card_view/03.jpg"><h5>App fgasgas Inc.</h5>
          </div>
          <div class="search-company search_auto_div" data-company="App asfyer Inc.">
            <img src="/assets/product_card_view/04.jpg"><h5>App asfyer Inc.</h5>
          </div>
          <%Company.all.each do |company|%>
            <%company.comlangs.each do |comlang|%>
              <div class="search-company search_auto_div" data-company="<%=comlang.name%>">
                <img src="<%=company.logo.url(:thumb)%>"><h5><%=comlang.name%></h5>
              </div>
            <%end%>
          <%end%>
        </div>
      </div>

      <script>
function search_auto_func(){
  if ($('#search_title').val() != ""){
          $(".search-company-name-drop").show();
          $(".search_auto_div").hide();
            var auto_count = 0
          $(".search_auto_div").each(function(){
            if ($(this).data("company").toLowerCase().indexOf($('#search_title').val().toLowerCase()) != -1 )
            {
                auto_count = auto_count + 1;
                $(this).show();             
                }
            
                });
            if (auto_count == 0){
              $(".search_auto_div").hide();
            }
          }else{
          $(".search_auto_div").hide();
          $(".search-company-name-drop").hide();
          
          }

}


      </script>



      <div class="header-right">
        <ul>
          <li><a href="#none">Biddings</a></li>
          <li><a href="#none">Product Results</a></li>
          
          <%if !(user_signed_in?)%>
            <li><a href="/users/sign_in" class="login-btn">Log in</a></li>
            <li><a href="/users/sign_up" class="signup-btn">Sign up</a></li>
          <%end%>
        </ul>
        <div class="log-name"><%if current_user != nil%><a href="/users/edit"><img src="<%if current_user.avatar_file_name == nil%><%fb_identity = current_user.identities.where(:provider => "facebook").first%><%if fb_identity != nil%>https://graph.facebook.com/v2.1/<%=fb_identity.uid%>/picture?type=square<%else%>/assets/main/log-img.png<%end%><%else%><%=current_user.avatar.url(:thumb)%><%end%>"> <%=current_user.first_name%> <%=current_user.last_name%> <img src="/assets/main/header-content.png" class="contents"></a><%end%>
          <div class="log-info-hover">
            <div class="log-info-account">
              <h5>My Account</h5>
              <a href="/users/edit">Profile</a>
              <a href="/users/company">Company Profile</a>
              <a href="/users/company_members">Member</a>
            </div>
            <div class="log-info-adv">
              <h5>Advertiser</h5>
              <a href="/home/account_dashboard">Dashboard</a><br>
              <a href="/advertisers/billings">Billing</a><br>
            </div>
            <div class="log-info-pub">
              <h5>Publisher</h5>
              <a href="#none">Projects</a><br>
              <a href="/home/account_manage_service">Manage Service</a><br>
              <a href="/publishers/create">Create Service</a><br>
              <a href="#none">Cash out</a>
            </div>
            <div class="log-info-footer">
              <a href="/users/sign_out">Log out</a>
              <!--<p><span>3 ietms</span> on your cart<img src="/assets/cart.png"></p>-->
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="nav" <%if @main %>style="display:none;"<%end%>>
    <div class="container">
      <!-- 단버튼 -->
      <div class="btn-group country">
        <button type="button" class="btn dropdown-toggle">
          Country <span class="caret"></span>
