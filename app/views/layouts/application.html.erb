  <div class="header">
    <div class="container" style="text-align: center">
      <a href="/home/main" class="logo" <%if @main%>style="visibility:hidden; float: left;"<%end%>><img src="/assets/logo-01.png"><h5 class="beta">Beta</h5></a>
      <div class="search-area">
        <div class="header-search">
          <span class="glyphicon glyphicon-search"></span>
          <input type="search" placeholder="Search Company"  autocomplete="off" id="search_title" onchange="search_auto_func();" onkeyup="search_auto_func();" name="search_title"  >
          <div class="search-drop search-company-name-drop" style="display:none;">
            <%Company.all.each do |company|%>
              <%company.comlangs.each do |comlang|%>
                <%if company.comlangs.where("name = ? AND id < ?", comlang.name, comlang.id).count == 0%>
                  <a href="/products/search_company/<%=company.id%>">
                    <div class="search-company search_auto_div" data-company="<%=comlang.name%>">
                      <img src="<%=company.logo.url(:medium)%>"><h5><%=comlang.name%></h5>
                    </div>
                  </a>
                <%end%>
              <%end%>
            <%end%>
          </div>
        </div>
      </div>

      <script>
$(document).mouseup(function (e)
{
    var container = $(".header-search");

    if (!container.is(e.target)) // ... nor a descendant of the container
    {
        $(".search-company-name-drop").hide();
    }
});

function search_auto_func(){
  if ($('#search_title').val() != ""){
          $(".search-company-name-drop").show();
          $(".search_auto_div").hide();
            var auto_count = 0
          $(".search_auto_div").each(function(){
            if ($(this).data("company").toLowerCase().indexOf($('#search_title').val().toLowerCase()) != -1 )
            {
                auto_count = auto_count + 1;
                $(this).show();             
                }
            
                });
            if (auto_count == 0){
              $(".search_auto_div").hide();
            }
          }else{
          $(".search_auto_div").hide();
          $(".search-company-name-drop").hide();
          
          }
}
      </script>
 
      <div class="header-right">
        <ul>
          <!-- <li><a href="#none">Biddings</a></li>-->
          <li><a href="/home/ready_search_detail">Project Results</a></li>
          <%if !(user_signed_in?)%>
            <li><a href="/users/sign_in" class="login-btn">Log In</a></li>
            <li><a href="/users/sign_up" class="signup-btn">Sign Up</a></li>
          <%end%>
        </ul>
        <div class="log-name"><%if current_user != nil%><a href="/users/edit"><img src="<%if current_user.avatar_file_name == nil%><%fb_identity = current_user.identities.where(:provider => "facebook").first%><%if fb_identity != nil%>https://graph.facebook.com/v2.1/<%=fb_identity.uid%>/picture?type=square<%else%>/assets/main/log-img.png<%end%><%else%><%=current_user.avatar.url(:thumb)%><%end%>"> <%=current_user.first_name%> <%=current_user.last_name%> <img src="/assets/main/menu.png" class="contents"></a><%end%>
