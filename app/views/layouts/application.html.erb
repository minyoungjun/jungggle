              <h5><a href="/users/members">Member</a></h5>
              <h5><a href="/users/ready_dashboard">Upgrade</a></h5>
            </div>
            <div class="log-info-adv">
              <h4 class="title">Advertiser</h4>
              <h5><a href="/home/ready_dashboard">Dashboard</a></h5><br>
              <h5><a href="/home/ready_billing">Billing</a></h5><br>
            </div>
            <div class="log-info-pub">
              <h4 class="title">Service Provider</h4>
              <h5><a href="/home/ready_project">Projects</a></h5><br>
              <%if user_signed_in? && current_user.is_admin%>
