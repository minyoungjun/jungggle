<!DOCTYPE html>
<html>
<head>
  <title>Jungggle</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="header container">
    <div class="search">
      <img src="/assets/main/search.png">
      <input type="text">
      
    </div>

    <ul>
      <li>Biddings</li>
      <li>Product Results</li>
      <li>UserName</li>
      <li>Cart</li>
      <%if user_signed_in?%>
        <li><%= link_to('sign out', destroy_user_session_path, :method => :delete) %></li>
      <%else%>
        <li><a href="/users/sign_in">sign in</a></li>
        <li><a href="/users/sign_up">sign up</a></li>
      <%end%>
    </ul>
  </div>

  <%= yield %>
  <div class="footer">
    <div class="container">
      <p> Copyrights 2014 "Jungggle LTD". All rights reserved. </p>
      <div class="footer-right">
        <ul>
          <li><a href="/home/aboutus">About us</a></li>
          <li><a href="/home/how">How it works</a></li>
          <li><a href="/home/faq">FAQ</a></li>
          <li><a href="/home/contact">Contact</a></li>
          <li><a href="#">Press</a></li>
          <li><a href="#">Terms</a></li>
          <li><a href="#">Privacy</a></li>
        </ul>
        <div class="footer-sns">
          <a href="#none">
            <img src="/assets/main/facebook.png">
            <img src="/assets/main/twitter.png">
          </a>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
