  <script>

function country_select_search(id){

  $("#country_text_search").html($("#country_" + id).html());
  $("#country_hidden").val(id);
}
function marketing_click_search(id){
  $("#marketing_text").html($("#marketing_li_search_" + id).html());
  $("#marketing_hidden").val(id);

    if(id > 10 && id < 60){
      $(".platform").show();
    }else{
      $(".platform").hide();
    
    }

}
function platform_click_search(id){
  $("#platform_text").html($("#platform_" + id).html());
  $("#platform_hidden").val(id);
}
function cost_key(){
  $(".cost").css("border-color", "#5bb33a");
  $(".cost-hover").css("display", "block");
  $(".cost-hover").css("color", "#5bb33a");
  $("#empty").css("display", "block");

}

  </script>

