  <div class="nav" <%if @main %>style="display:none;"<%end%>>
<%if !(@main) %>
<form action="/products/search" method="get" id="search_form" style="visibility: hidden;">
  <input type="hidden" name="country" id="country_hidden" value="0">
  <input type="hidden" name="marketing" id="marketing_hidden" value="1">
  <input type="hidden" name="platform" id="platform_hidden" value="1">
  <input type="hidden" name="cost_from" id="cost_from_hidden" value="0">




    <div class="container">
      <!-- 단버튼 -->
      <div class="btn-group country">
        <button type="button" class="btn dropdown-toggle country-btn"><span id="country_text_search">Country</span><span class="glyphicon glyphicon-chevron-down"></span>
        </button>
        <div class="country-down">
          <div class="title" id="country_0" onclick="country_select_search(0);">All</div>
          <ul class="country-down-content">
            <hr>
            <%Country.able_list.each do |country|%>
              <li id="country_<%=country.id%>" onclick="country_select_search(<%=country.id%>);"><%=country.name%></li>
            <%end%>
            <hr>
          </ul>
          <div class="other">
            Other Countries..
            <div class="other-tooltip">
              <img src="/assets/my_account/hover_pointer.svg">
              Countries are still adding whenever Publisher create service on particular country.
            </div>
          </div>
        </div>
      </div>
      <div class="btn-group marketing" style="z-index: 101;">
        <button type="button" class="btn dropdown-toggle marketing-btn"><span id="marketing_text">Marketing Type</span> <span class="glyphicon glyphicon-chevron-down"></span></button>
          <div class="marketing-down">

              <ul>
                <%Marketingtype.where(:filter => 1).each do |marketing1|%>
                  <%if marketing1.name == "All"%>
                    <li class="title" onclick="marketing_click_search(<%=marketing1.id%>);" id="marketing_li_search_<%=marketing1.id%>" data-marketing ="<%=marketing1.name%>">All</li><hr>
                  <%else%>
                    <li class="division" >
                    <span  id="marketing_li_search_<%=marketing1.id%>" data-marketing ="<%=marketing1.name%>" onclick="marketing_click_search(<%=marketing1.id%>);"><%=marketing1.name%></span>
                      <%if marketing1.child_types.count != 0%>
                        <div class="division-hover <%if marketing1.name == "AD"%>division-hover-plus<%end%> " style="min-height:162px;">
                          <ul>
                            <% marketing1.child_types.order(:index_number).each do |marketing2|%>
                              <%if marketing2.filter == 2%>
                                <ul>
                                  <li class="title" onclick="marketing_click_search(<%=marketing2.id%>);" data-marketing ="<%=marketing2.name%>" id="marketing_li_search_<%=marketing2.id%>" ><%=marketing2.name%></li>
                                  <%if marketing2.child_types.count != 0%>
                                    <%marketing2.child_types.order(:index_number).each do |marketing3|%>
                                      <%if marketing3.filter != 4%>
                                        <li onclick="marketing_click_search(<%=marketing3.id%>);"id="marketing_li_search_<%=marketing3.id%>"data-marketing ="<%=marketing3.name%>" ><%=marketing3.name%></li>
                                      <%end%>
                                    <%end%>
                                  <%end%>
                                </ul>
                              <%else%>
                                <li onclick="marketing_click_search(<%=marketing2.id%>);"id="marketing_li_search_<%=marketing2.id%>" data-marketing ="<%=marketing2.name%>"><%=marketing2.name%></li>
                            <%end%>
                          <%end%>
                        </ul>
                    </div>
                  <%end%>
                  </li>
                <%end%>
              <%end%>
            </ul>
        </div>
      </div>

      <div class="btn-group" style="margin-left: 2px;">
        <div class="cost">
          <input type="text" value="Cost" id="fake_cost">
          <div class="cost-hover">
            $ | <input type="text" name="cost_from" onkeyup="cost_key();" style="padding-left: 0;">to&nbsp;
            $ | <input type="text" name="cost_to" onkeyup="cost_key();" style="padding-left: 0;">
          </div>
        </div>         
      </div>
        <button type="button" class="btn nav-btn" onclick="$('#search_form').submit();">Jungggling<img src="/assets/main/search_logo.png"></button>
      </form>

    </div>
  <%end%>
</div>
