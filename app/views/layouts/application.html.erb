</head>
<body>
  <div class="header">
    <div class="container">
      <a href="/home/main" class="logo"><img src="/assets/main/header-logo.png"<%if @main%>style="opacity:0;"<%end%>></a>
      <div class="header-search">
        <span class="glyphicon glyphicon-search"></span>
        <input type="text">
      </div>

      <ul>
        <li><a href="#none">Biddings</a></li>
        <li><a href="#none">Product Results</a></li>
        
        <%if user_signed_in?%>
          <li><%= link_to('sign out', destroy_user_session_path) %></li>
        <%else%>
          <li><a href="/users/sign_in">sign in</a></li>
          <li><a href="/users/sign_up">sign up</a></li>
        <%end%>
        <li class="log-name"><%if current_user != nil%><a href="/users/edit"><img src="<%if current_user.avatar_file_name == nil%><%fb_identity = current_user.identities.where(:provider => "facebook").first%><%if fb_identity != nil%>https://graph.facebook.com/v2.1/<%=fb_identity.uid%>/picture?type=square<%else%>/assets/main/log-img.png<%end%><%else%><%=current_user.avatar.url(:thumb)%><%end%>"> <%=current_user.first_name%> <%=current_user.last_name%> <img src="/assets/main/header-content.png" class="contents"></a><%end%></li>
      </ul>
    </div>
  </div>
  <div class="nav" <%if @main %>style="display:none;"<%end%>>
    <div class="container">
      <!-- 단버튼 -->
      <div class="btn-group">
        <button type="button" class="btn dropdown-toggle" data-toggle="dropdown">
          Country <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" role="menu">
          <%Country.all.each do |country|%>
            <li><a href="#"><%=country.name%></a></li>
          <%end%>
        </ul>
      </div>
      <div class="btn-group">
        <button type="button" class="btn dropdown-toggle" data-toggle="dropdown">
          Marketin type <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" role="menu">
          <li><a href="#">Action</a></li>
          <li><a href="#">Another action</a></li>
          <li><a href="#">Something else here</a></li>
          <li class="divider"></li>
          <li><a href="#">Separated link</a></li>
        </ul>
      </div>
      <div class="btn-group">
        <button type="button" class="btn dropdown-toggle" data-toggle="dropdown">
          Cost <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" role="menu">
          <li><a href="#">Action</a></li>
          <li><a href="#">Another action</a></li>
          <li><a href="#">Something else here</a></li>
          <li class="divider"></li>
          <li><a href="#">Separated link</a></li>
        </ul>
      </div>
      <button type="button" class="btn nav-btn">Jungggling<img src="/assets/main/search_logo.png"></button>
    </div>
  </div>

  <%= yield %>
  <div class="footer">
    <div class="container">
      <p> Copyrights 2014 "Jungggle LTD". All rights reserved. </p>
      <div class="footer-right">
        <ul>
          <li><a href="/home/aboutus">About us</a></li>
          <li><a href="/home/how">How it works</a></li>
          <li><a href="/home/faq">FAQ</a></li>
          <li><a href="/home/contact">Contact</a></li>
          <li><a href="#">Press</a></li>
          <li><a href="#">Terms</a></li>
          <li><a href="#">Privacy</a></li>
        </ul>
        <div class="footer-sns">
          <a href="#none">
            <img src="/assets/main/facebook.png">
            <img src="/assets/main/twitter.png">
          </a>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
