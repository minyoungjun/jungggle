      <div class="btn-group marketing">
        <button type="button" class="btn dropdown-toggle">Marketing type <span class="caret"></span></button>
        <div class="marketing-down">
          <ul>
            <li class="title">All</li><hr>
            <li class="division">
              Ads
              <div class="division-hover">
                <ul>
                  <li class="title">Online</li>
                  <li>CPM</li>
                  <li>Video</li>
                  <li>Banner (period)</li>
                  <li>Direct Email</li>
                  <li>Text ad</li>
                  <li>SEO</li>
                  <li>SNS</li>
                </ul>
                <ul>
                  <li class="title">Mobile</li>
                  <li>CPM</li>
                  <li>Video</li>
                  <li>Banner (period)</li>
                  <li>Direct Email</li>
                  <li>Text ad</li>
                  <li>SEO</li>
                  <li>SNS</li>
                </ul>
                <ul>
                  <li class="title">Offline</li>
                  <li>CPM</li>
                  <li>Video</li>
                  <li>Banner (period)</li>
                  <li>Direct Email</li>
                  <li>Text ad</li>
                  <li>SEO</li>
                  <li>SNS</li>
                </ul>
              </div>
            </li>
            <li>Promotion</li>
            <li>Management</li>
            <li>Test</li>
            <li>Material</li>
          </ul>
        </div>
      </div>

      <div class="btn-group">
        <div class="cost">
          <input type="text" placeholder="Cost">
          <div class="cost-hover">
            $ | <input type="text">to&nbsp;
            $ | <input type="text">
          </div>
        </div>         
      </div>
      <button type="button" class="btn nav-btn">Jungggling<img src="/assets/main/search_logo.png"></button>
      <!--<a href="/products/cart"><img src="/assets/cart.png" class="nav-cart"></a>-->
    </div>
  </div>
  <%if user_signed_in? && current_user.email_confirmed && current_user.confirmed_at == nil%>
    <h5 class="resend">Please confirm your email address, a confirmation message was sent to <%=current_user.email%> <span class="underline">Resend confrimation</span></h5>
  <%end%>
  <%#
    <h5 class="account-update">Your account has been updated.</h5>
  %>
  <%= yield %>
  <div class="footer">
    <div class="container">
      <p> Copyrights 2014 "Jungggle LTD". All rights reserved. </p>
      <div class="footer-right">
        <ul>
          <li><a href="/home/aboutus">About</a></li>
          <li><a href="/home/how">How it works</a></li>
          <li><a href="/home/faq">FAQ</a></li>
          <li><a href="/home/contact">Contact</a></li>
          <li><a href="#">Press</a></li>
          <li><a href="#">Terms</a></li>
          <li><a href="#">Privacy</a></li>
        </ul>
        <div class="footer-sns">
          <a href="#none">
            <img src="/assets/main/facebook.png">
            <img src="/assets/main/twitter.png">
