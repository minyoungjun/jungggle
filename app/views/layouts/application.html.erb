<script>

$(document).ready(function(){
    $(function(){
        height_match();
      });
          
    });

function height_match(){
  $("#left").css("height", "");
  $(".right-height").css("height", "");
  if ($("#left").outerHeight() < $(".right-height").outerHeight()){
            $("#left").outerHeight($(".right-height").outerHeight());
      }
        else{
            $(".right-height").outerHeight($("#left").outerHeight());
      }
}
