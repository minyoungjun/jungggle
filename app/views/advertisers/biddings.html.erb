<div class="account">
  <div class="container">
    <%=render "/home/left"%>
    <div class="right-ad-biddings">
      <h1>Current Biddings</h1>
      <div class="sub-title">
        <h3>Pick button will be vanish 1 week after the deadline</h3>
        <button type="button" class="btn btn-default" onclick="location.href='/advertisers/new_bidding';"><img src="/assets/main/search_logo.png">Create Bidding <span>$99</span></button>
      </div>
      <table>
        <tr class="table-header">
          <td>Service Name</td>
          <td>Budget (max)</td>
          <td>Marketing Type</td>
          <td>Country</td>
          <td>Deadline</td>
          <td># of entry</td>
          <td>Status</td>
        </tr>
        <%@biddings.each do |bidding|%>
          <tr class="table-body">
            <td class="name"><%=bidding.name%></td>
            <td class="budget"><%=bidding.budget%></td>
            <td><%=bidding.marketingtype.name%></td>
            <td><%=bidding.country.name%></td>
            <td><%=bidding.deadline%></td>
            <td>4</td>
            <td class="status">on</td>
          </tr>
        <%end%>
      </table>
    </div>
  </div>
</div>















